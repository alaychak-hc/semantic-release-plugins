{
  "name": "@alaychak-hc/semantic-release-plugins-ms-teams",
  "version": "0.0.009",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:alaychak-hc/semantic-release-plugins.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "prepublish": "pnpm build",
    "build": "tsc --p ./tsconfig.build.json",
    "build:pack": "pnpm build && pnpm pack && pnpm clean",
    "clean": "rimraf ./dist",
    "publish:teams": "pnpm publish --force --no-git-checks",
    "publish:teams:local": "pnpm publish --dry-run --force --no-git-checks"
  },
  "devDependencies": {
    "@types/node": "^20.9.5",
    "@types/semantic-release": "^20.0.6",
    "node-emoji": "^2.1.3",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.8",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "aggregate-error": "^5.0.0",
    "mdast-util-to-markdown": "^2.1.0",
    "remark": "^15.0.1"
  }
}
