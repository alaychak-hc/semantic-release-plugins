{
  "name": "@alaychak-hc/semantic-release-plugins-ms-teams",
  "version": "0.0.0011",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:alaychak-hc/semantic-release-plugins.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "prepublish": "pnpm build",
    "build": "tsc --p ./tsconfig.build.json",
    "build:pack": "pnpm build && pnpm copy:templates && pnpm pack && pnpm clean",
    "clean": "rimraf ./dist",
    "copy:templates": "cpy ./lib/templates/*.hbs ./dist/templates",
    "publish:teams": "pnpm publish --force --no-git-checks",
    "publish:teams:local": "pnpm publish --dry-run --force --no-git-checks"
  },
  "devDependencies": {
    "@types/node": "^22.9.1",
    "@types/semantic-release": "^20.0.6",
    "cpy-cli": "^5.0.0",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.8",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "aggregate-error": "^5.0.0",
    "express-handlebars": "^8.0.1",
    "remark": "^15.0.1",
    "slash": "^5.1.0"
  }
}
